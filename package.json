    "vscode:prepublish": "npm run pretest",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "find-parent-dir": "^0.3.0",
    "mkdirp": "^0.5.1",
    "mkdirpsync": "^0.1.0",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.6",
    "jest": "^22.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.26"
  },
  "engines": {
    "vscode": "^1.20.0"
  }
